{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "generate2": "apollo-codegen generate ./apollo/query.ts --schema=schema.json --target=typescript --output=./apollo/query-types.ts --tagName=gql --addTypename ",
    "introspect-schema2": "apollo-codegen introspect-schema http://localhost:3000/api/graphql --output schema.json",
    "download-schema": "apollo client:download-schema --endpoint=http://localhost:3000/api/graphql graphql-schema.json",
    "generate": "apollo client:codegen --includes=./apollo/**/*.ts --localSchemaFile=graphql-schema.json --target=typescript --tagName=gql"
  },
  "dependencies": {
    "@apollo/client": "^3.3.14",
    "apollo-datasource": "^0.7.3",
    "apollo-datasource-rest": "^0.11.0",
    "apollo-server-micro": "^2.22.2",
    "graphql": "^14.6.0",
    "isemail": "^3.2.0",
    "kuroshiro": "^1.1.2",
    "next": "latest",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-full-screen": "^1.0.2",
    "react-typing-game-hook": "^1.2.1",
    "sequelize": "^6.6.2",
    "sqlite3": "^5.0.2",
    "swr": "^0.5.3",
    "use-sound": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "apollo": "^2.32.5",
    "autoprefixer": "^10.2.5",
    "file-loader": "^6.2.0",
    "postcss": "^8.2.8",
    "tailwindcss": "^2.0.4",
    "typescript": "4.0",
    "url-loader": "^4.1.1"
  },
  "resolutions": {
    "graphql": "15.4.0"
  },
  "license": "MIT"
}
